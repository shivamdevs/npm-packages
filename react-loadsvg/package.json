{
  "name": "react-loadsvg",
  "version": "1.0.5",
  "description": "The LoadSVG component is a simple and customizable SVG-based loading spinner.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "typings": "./dist/esm/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "postpublish": "npm run clean",
    "compile": "npm run clean && npm run compile:esm && npm run compile:cjs",
    "compile:esm": "tsc",
    "compile:cjs": "tsc --module commonjs --outDir dist/cjs",
    "clean": "rimraf dist"
  },
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shivamdevs/npm-packages/tree/main/react-loadsvg"
  },
  "keywords": [
    "react",
    "react-loading",
    "loading-icon",
    "svg-loader",
    "svg-icon",
    "load",
    "loading",
    "load-icon"
  ],
  "author": "ShivamDevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shivamdevs/npm-packages/issues",
    "email": "shivam0dewangan@gmail.com"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "eslint": "^8.48.0",
    "react": ">=18",
    "react-dom": ">=18",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "homepage": "https://github.com/shivamdevs/npm-packages/tree/main/react-loadsvg#readme",
  "dependencies": {
    "tslib": "^2.6.2"
  }
}
